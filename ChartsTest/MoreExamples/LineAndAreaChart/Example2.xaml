<UserControl x:Class="ChartsTest.MoreExamples.LineAndAreaChart.Example2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChartsTest.MoreExamples.LineAndAreaChart"
             xmlns:charts="clr-namespace:LiveCharts.Charts;assembly=LiveCharts"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             Loaded="Example2_OnLoaded">
    <Grid>
        <charts:LineChart Name="Chart" LineType="Polyline"></charts:LineChart>
    </Grid>
</UserControl>
